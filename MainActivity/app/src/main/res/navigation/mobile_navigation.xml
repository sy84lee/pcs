<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_hash">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.sy.pcs.ui.cryptography.CryptographyFragment"
        android:label="@string/title_file"
        tools:layout="@layout/fragment_aes" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.sy.pcs.ui.text.TextFragment"
        android:label="@string/title_text"
        tools:layout="@layout/fragment_text" />

    <fragment
        android:id="@+id/navigation_hash"
        android:name="com.sy.pcs.ui.hash.HashFragment"
        android:label="HASH"
        tools:layout="@layout/fragment_hash" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.sy.pcs.ui.notifications.NotificationsFragment"
        android:label="@string/title_none"
        tools:layout="@layout/fragment_notifications" />
</navigation>